  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.432.0",
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.0",
    "cookie-session": "^2.0.0-beta.2",
    "cors": "^2.8.5",
    "express": "^4.15.3",
    "migrate-mongoose": "^3.2.2",
    "mongoose": "^4.13.18",
    "nodemon": "^1.18.10",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "path-parser": "^2.0.2",
    "puppeteer": "^1.0.0",
    "redis": "^2.8.0",
    "uuid": "^3.2.1"
  }
}
